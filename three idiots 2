import java.util.*;

public class Solution {
    
public static void main(String[]args) {
    int x1;
    int y1;
    int x2;
    int y2;
    double a;
    double b;
    Scanner sc=new Scanner(System.in);
    x1=sc.nextInt();
    y1=sc.nextInt();
    x2=sc.nextInt();
    y2=sc.nextInt();
    a=((x1+x2)/2.0);
    b=((y1+y2)/2.0);
      System.out.printf("Binoy's house is located at (%.1f,%.1f)",a,b);



}
}
