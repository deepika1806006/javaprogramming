import java.util.*;
class solution {
public static void main(String[]args){
    floatimport java.util.*;
public a;
    Scanner sc=new Scanner(System.in);
    a=sc.nextFloat();
    System.out.printf("%.006f \n",a);
    System.out.printf("%.004f \n",a);
    System.out.printf("%.2f \n",a);
    System.out.printf("%.0f \n",a);
}}
